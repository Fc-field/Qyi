{
    "name": "qyi",
    "private": true,
    "version": "0.0.0",
    "main": "dist-electron/index.js",
    "scripts": {
        "dev": "vite",
        "build": "rimraf dist-electron && vite build && electron-builder",
        "preview": "vite preview"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "element-plus": "^2.4.1",
        "pinia": "^2.1.7",
        "vue": "^3.3.4",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@types/node": "^20.8.9",
        "@vitejs/plugin-vue": "^4.2.3",
        "electron": "^27.0.2",
        "electron-builder": "^24.6.4",
        "electron-devtools-installer": "^3.2.0",
        "rimraf": "^5.0.5",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "typescript": "^5.0.2",
        "unplugin-auto-import": "^0.16.7",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.4.5",
        "vite-plugin-electron": "^0.14.1",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vue-tsc": "^1.8.5"
    },
    "build": {
        "electronDownload": {
            "mirror": "https://npm.taobao.org/mirrors/electron/"
        },
        "appId": "com.example.app",
        "productName": "测试",
        "asar": true,
        "directories": {
            "buildResources": "build",
            "output": "dist"
        },
        "mac": {
            "category": "public.app-category.developer-tools",
            "target": [
                "dmg",
                "zip"
            ],
            "icon": "build/icon.icns"
        },
        "dmg": {
            "background": "build/background.tiff or build/background.png",
            "title": "标题",
            "icon": "build/icon.icns"
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "build/installerIcon_120.ico",
            "uninstallerIcon": "build/uninstallerIcon_120.ico",
            "installerHeaderIcon": "build/installerHeaderIcon_120.ico",
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "license": "LICENSE.txt"
        }
    }
}
